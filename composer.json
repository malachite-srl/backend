{
    "keywords": ["framework", "laravel", "amethyst"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2",
        "amethyst/account": "0.2.*",
        "amethyst/action": "^0.2.1",
        "amethyst/attribute-schema": "0.2.*",
        "amethyst/authentication": "0.2.*",
        "amethyst/data-schema": "0.2.*",
        "amethyst/data-view": "0.2.*",
        "amethyst/group": "0.2.*",
        "amethyst/owner": "0.2.*",
        "amethyst/permission": "0.2.*",
        "amethyst/relation-schema": "0.2.*",
        "amethyst/setting": "0.2.*",
        "amethyst/user": "0.2.*",
        "barryvdh/laravel-cors": "^0.11.2",
        "benconstable/laravel-localize-middleware": "^1.2",
        "bugsnag/bugsnag-laravel": "^2.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "imanghafoori/eloquent-relativity": "dev-dev-non-static",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.0",
        "laravel/tinker": "^1.0",
        "matthewbdaly/laravel-etag-middleware": "^1.2",
        "mingalevme/illuminate-uqueue": "^1.1",
        "predis/predis": "^1.1",
        "swooletw/laravel-swoole": "^2.5"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "pre-install-cmd": [
            "@php -r \"file_exists('bootstrap/cache/map.php') && unlink('bootstrap/cache/map.php');\""
        ],
        "pre-update-cmd": [
            "@php -r \"file_exists('bootstrap/cache/map.php') && unlink('bootstrap/cache/map.php');\""
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan cache:clear",
            "@php artisan mapper:generate",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:railken/eloquent-relativity.git"
        }
    ]
}
