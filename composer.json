{
    "keywords": ["framework", "laravel", "amethyst"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "amethyst/account": "0.1.*",
        "amethyst/admin": "0.1.*",
        "amethyst/attendance": "0.1.*",
        "amethyst/attribute": "0.1.*",
        "amethyst/authentication": "0.1.*",
        "amethyst/company": "0.1.*",
        "amethyst/config": "0.1.*",
        "amethyst/contact": "0.1.*",
        "amethyst/customer": "0.1.*",
        "amethyst/data-builder": "0.1.*",
        "amethyst/data-view": "0.1.*",
        "amethyst/email-log": "0.1.*",
        "amethyst/email-sender": "0.1.*",
        "amethyst/employee": "0.1.*",
        "amethyst/event-logger": "0.1.*",
        "amethyst/exporter": "0.1.*",
        "amethyst/file": "0.1.*",
        "amethyst/file-generator": "0.1.*",
        "amethyst/importer": "0.1.*",
        "amethyst/invoice": "0.1.*",
        "amethyst/issue": "0.1.*",
        "amethyst/listener": "0.1.*",
        "amethyst/notification": "0.1.*",
        "amethyst/notification-sender": "0.1.*",
        "amethyst/office": "0.1.*",
        "amethyst/owner": "0.1.*",
        "amethyst/permission": "0.1.*",
        "amethyst/post": "0.1.*",
        "amethyst/price": "0.1.*",
        "amethyst/product": "0.1.*",
        "amethyst/project": "0.1.*",
        "amethyst/schedule": "0.1.*",
        "amethyst/setting": "0.1.*",
        "amethyst/supplier": "0.1.*",
        "amethyst/taxonomy": "0.1.*",
        "amethyst/template": "0.1.*",
        "amethyst/token": "0.1.*",
        "amethyst/user": "0.1.*",
        "amethyst/warehouse": "0.1.*",
        "amethyst/work": "0.1.*",
        "barryvdh/laravel-cors": "^0.11.2",
        "benconstable/laravel-localize-middleware": "^1.2",
        "bugsnag/bugsnag-laravel": "^2.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "hanneskod/classtools": "~1.0",
        "imanghafoori/eloquent-relativity": "^1.0",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^6.0",
        "laravel/tinker": "^1.0",
        "matthewbdaly/laravel-etag-middleware": "^1.2",
        "predis/predis": "^1.1",
        "swooletw/laravel-swoole": "^2.5",
        "tecnickcom/tcpdf": "^6.2"
    },
    "require-dev": {
        "amethyst/foo": "^0.1.8",
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpstan/phpstan": "^0.10.2",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan mapper:generate"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
